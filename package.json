{
  "name": "cbjohnson.info",
  "scripts": {
    "co-gh-pages": "git checkout gh-pages",
    "co-master": "git checkout master",
    "commit-dist" : "git add --all ./ && git commit --all --message 'gh-pages update'",
    "dist": "gulp clean && gulp dist",
    "dry-publish-dist": "npm run write-dist && npm run commit-dist",
    "gh-pages-publish": "npm run dist && npm run co-gh-pages && npm run publish-dist && npm run co-master",
    "gh-pages-stage": "npm run dist && npm run co-gh-pages && npm run dry-publish-dist && npm run co-master",
    "lint": "stylelint src/stylesheets/**/*.css",
    "publish-dist": "npm run write-dist && npm run commit-dist && npm run push-dist",
    "push-dist": "git push origin gh-pages",
    "start": "http-server public",
    "write-dist": "cp -rf public/* ./ && rm -rf ./public"
  },
  "dependencies": {
    "autoprefixer": "6.5.0",
    "css-mqpacker": "5.0.1",
    "del": "2.2.0",
    "gulp": "3.9.1",
    "gulp-atomizer": "1.0.6",
    "gulp-concat": "2.6.0",
    "gulp-if": "2.0.1",
    "gulp-imagemin": "3.0.3",
    "gulp-minify-css": "1.2.4",
    "gulp-postcss": "6.2.0",
    "gulp-rev": "7.1.2",
    "gulp-rev-replace": "0.4.3",
    "gulp-sourcemaps": "1.6.0",
    "gulp-useref": "3.1.2",
    "normalize.css": "4.2.0",
    "octicons": "3.1.0",
    "postcss-calc": "5.3.1",
    "postcss-color-function": "2.0.1",
    "postcss-color-gray": "3.0.0",
    "postcss-css-variables": "0.6.0",
    "postcss-custom-media": "5.0.1",
    "postcss-custom-selectors": "3.0.0",
    "postcss-import": "8.1.2",
    "postcss-media-minmax": "2.1.2",
    "postcss-merge-rules": "2.0.10",
    "postcss-modular-scale": "2.5.2",
    "postcss-nested": "1.0.0",
    "postcss-responsive-type": "0.4.0",
    "postcss-strip-units": "1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "7.0.0",
    "http-server": "0.9.0",
    "standard": "8.3.0",
    "stylelint": "7.3.1",
    "stylelint-config-standard": "13.0.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  }
}
